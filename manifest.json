{
  "manifest_version": 2,

  "name": "Web Development Sidekick",
  "description": "This is a beta extension developed by Mike Droy.",
  "version": "1.0",
  "background": {"page": "background.html"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
